@model MenuItem

<h2>Menu Item Details</h2>

<dl class="row">
    <dt class="col-sm-2">Item Name</dt>
    <dd class="col-sm-10">@Model.ItemName</dd>

    <dt class="col-sm-2">Price</dt>
    <dd class="col-sm-10">@Model.Price</dd>

    <dt class="col-sm-2">Restaurant</dt>
    <dd class="col-sm-10">@Model.Restaurant.Name</dd>

    <!-- Add more details for other properties -->
</dl>